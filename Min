public class Min {
    int arr[] = {25, 37, 29};            // 1

    void min() {
        int min = arr[0];
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] < min) {
                min = arr[i];
            }

        }
        System.out.println(  " The smallest value is " + min);
    }

}

class Demomin {
    public static void main(String[] args) {
        Min demomain = new Min();
        demomain.min();
    }
}
