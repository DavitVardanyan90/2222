public class Pentagonal {
    void pentagonal() {             //  6
        int first = 1;
        int second = 5;
        int pent = second + second - first + 3;

        for (int i = 0; i < 50; i++) {

            first = second;
            second = pent;
            pent = second + second - first + 3;

            System.out.println(pent);
        }
    }

}

class Demo {
    public static void main(String[] args) {
        Pentagonal demomain = new Pentagonal();
        demomain.pentagonal();
    }
}
