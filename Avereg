public class Avereg {
    int arr[] = {25, 45, 65};           // 2

    void midl() {
        int number = 0;
        for (int i = 0; i < arr.length; i++) {

            number += arr[i];
        }
        System.out.println(  "The middle character in the string " +  number / arr.length);
    }
}

class MAavereg {
    public static void main(String[] args) {
        Avereg avereg = new Avereg();
        avereg.midl();

    }
}
